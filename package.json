{
  "name": "jaect",
  "version": "0.5.3",
  "description": "Jade compiler for Facebook React",
  "authors": [
    "Michael Phan-Ba <michael@mikepb.com>",
    "thr0w <thr0w@thr0w.lol>"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/wujunhong/jade-react-compiler-x"
  },
  "main": "index.js",
  "bin": {
    "jade-react-compiler": "./bin/jade-react-compiler.js"
  },
  "dependencies": {
    "escodegen-jsx": "0.1.0-1.4.2dev",
    "esprima-fb": "^15001.1001.0-dev-harmony-fb",
    "jade": "^1.9.2",
    "react": "^0.13.3"
  },
  "devDependencies": {
    "ast-types": "^0.8.9",
    "chai": "^3.2.0",
    "espree": "^2.2.4",
    "js-beautify": "*",
    "markdown": "*",
    "mocha": "*",
    "source-map": "^0.4.4",
    "stylus": "*"
  },
  "scripts": {
    "test": "mocha --reporter spec",
    "coverage": "mocha --require blanket -R html-cov -g ReactCases> coverage/coverage.html"
  },
  "config": {
    "blanket": {
      "pattern": "///[\\w-]+\\.js$/",
      "data-cover-never": [
        "node_modules"
      ],
      "data-cover-reporter-options": {
        "shortnames": true
      }
    }
  },
  "license": "MIT"
}
